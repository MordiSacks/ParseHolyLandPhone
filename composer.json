{
  "name": "parse-holy-land-phone/parse-phone",
  "description": "Parse israeli/Palestinian phone numbers",
  "keywords": [
    "phone number",
    "phone",
    "Israel",
    "Palestine installation",
    "Holy Land"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mordi Sacks"
    }
  ],
  "require": {
    "php": ">=7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "ParseHolyLandPhone\\": "src/"
    }
  },
  "require-dev": {
    "laravel/pint": "^1.17",
    "pestphp/pest": "^2.35",
    "phpstan/phpstan": "^1.11"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
